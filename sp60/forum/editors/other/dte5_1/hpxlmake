ccxl  edc,edo,$NULL;INFO="-DHPXL"
ccxl  blockc,blocko,$NULL;INFO="-DHPXL"
ccxl  findrepc,findrepo,$NULL;INFO="-DHPXL"
ccxl  hwindc,hwindo,$NULL;INFO="-DHPXL"
ccxl  utilsc,utilso,$NULL;INFO="-DHPXL"
ccxl  windowc,windowo,$NULL;INFO="-DHPXL"
ccxl  hwhpxlc,hwhpxlo,$NULL;INFO="-DHPXL"
link  from=edo,blocko,findrepo,hwindo,utilso,windowo,&
      hwhpxlo;&
      rl=^ccstdrl.lib.sys;&
      to=dte
copy dte,dtej
altsec dte;(R,X:ANY;R,W,X,A,L:CR)
altsec dtej;(R,X:ANY;R,W,X,A,L:CR)
altsec dtehelp;(R:ANY;R,W,A,L:CR)
release dte
release dtej
release dtehelp
