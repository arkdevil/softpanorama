# Make DTECGA.EXE to run on "snowy" CGA computers

OBJECTS = ed.obj utils.obj findrep.obj block.obj hwind.obj hwibmcga.obj \
          window.obj

.c.obj:
    tcc -mc -w -d -K -k- -G -O -Z -c -DPROTO $<

dtecga.exe: $(OBJECTS)
    tcc -mc -w -K -k- -edtecga $(OBJECTS)

ed.obj:               common.h utils.h findrep.h block.h window.h
utils.obj:            common.h utils.h
findrep.obj:          common.h utils.h findrep.h
window.obj:           common.h utils.h window.h
block.obj:            common.h utils.h block.h
hwind.obj:            common.h hwdep.h
hwibmcga.obj:         common.h hwdep.h version.h
