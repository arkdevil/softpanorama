		Макрокоманда FOR   ENDFOR	(продолжение)

		Использование макрокоманд	CONNINUE и BREAK


	CONTINUE - вызывает переход к следующей итерации цикла без
		   завершения предыдущей

	BREAK	 - вызывает немедленный выход из цикла

Пример:
	xor	ax,ax
	lea	bx,TABL

	FOR	di 0 100 5		;<-------
					;	 !
	mov	cx,[bx+di]		;	 !
					;	 !
		if$	cx e 0		;	 !
		break	       ;----		 !
		endif$	       ;    !		 !
			       ;    !		 !
		if$	,s,,   ;    !		 !
		continue       ;    ! -----------
		endif$	       ;    !
			       ;    !
	add	ax,cx	       ;    !
			       ;    !
	ENDFOR		       ;<---

Листинг:
		mov	di,0
		sub	di,5
	$$A1n:
		cmp	di,100
		jz     $$A1e
		add	di,5

		mov	cx,[bx+di]

		cmp	cx,0
		jne    $$B2el
		jmp	short	$$A1e
	$$B2el:
		jns    $$B3el
		jmp	short	$$A1n
	$$B3el:

		add	ax,cx
		jmp	short $$A1n
	$$A1e:






