		7. Вложения макрокоманд.

	Количество вложений макрокоманд  не ограничивается  !!!
	Ограничено лишь общее число макрокоманд типа begin
	( FOR,DO,SWITCH,WHILE )  числом 65535, которое является
	наибольшим целым без знака,помещающимся в 2 байта.
Пример, демонстрирующий возможности
	вложения макрокоманд.

	DO
	xor	ax,ax
	int	16h		; ввод символа с клавиатуры

		SWITCH	al,<'A','B','C','D',0>
		case	1
			call	sub1
			break
		case	2
			call	sub2
			break
		case	3
			call	sub3
			break
		case	4
			call	sub4
			break
		case	5
			switch	ah,<104,105,106>	 ; анализ EXTENDED кодов
			case	1			 ; ALT-F1,ALT-F2,ALT-F3
			call	sub5
			break
			case	2
			call	sub6
			break
			case	3
			call	sub7
			endcase
		endcase
	UNTIL	al  e 27	; ESC
Листинг:
$$A1n:
	xor	ax,ax
	int	16h		; ввод символа с клавиатуры
	cmp	al,'A'
	jz	$$A21c
	cmp	al,'B'
	jz	$$A22c
	cmp	al,'C'
	jz	$$A23c
	cmp	al,'D'
	jz	$$A24c
	cmp	al,0
	jz	$$A25c
	jmp	short	$$A2e
$$A21c:
			call	sub1
	jmp	short	$$A2e
$$A22c:
			call	sub2
	jmp	short	$$A2e

				     _15_

		Вложения макрокоманд  ( продолжение )
$$A23c:
			call	sub3
	jmp	short	$$A2e
$$A24c:
			call	sub4
	jmp	short	$$A2e
$$A25c:
				       ; анализ EXTENDED кодов
	cmp	ah,104
	jz	$$A31c
	cmp	ah,105
	jz	$$A32c
	cmp	ah,106
	jz	$$A33c
	jmp	short	$$A3e
$$A31c:
			call	sub5
	jmp	short	$$A3e
$$A32c:
			call	sub6
	jmp	short	$$A3e
$$A33c:
			call	sub7
$$A3e:
$$A2e:
				; ESC
	cmp	al,27
	jne	$$A1n
$$A1e:

