Procedure SetPaletter ( Pal : TPaletter );
  { Установка палитры пользователя }
Var
  Rg : Registers;
Begin
  Rg.AX := $1002;
  Rg.ES := Seg ( Pal );
  Rg.DX := Ofs ( Pal );
  Intr ( $10, Rg );
End; { Procedure SetPaletter ( Pal : TPaletter ) }
{----------------------------------------------------------}
Procedure SetStandartPaletter;
  { Установка стандартной палитры }
Var
  Pal : TPaletter;
  Index : Byte;
Begin
  For Index := 0 to 15 Do if Index < 8 then
    Pal [ Index ] := Index
  else
    Pal [ Index ] := ( Index Mod 8 ) + $38;
  Pal [ 6 ] := Pal [ 6 ] + $10;
  Pal [ 16 ] := 0;
  SetPaletter ( Pal );
End; { Procedure SetStandartPaletter }
{----------------------------------------------------------}
Procedure SetSecondFontPaletter;
  { Установка палитры при использовании двух шрифтов }
Var
  Pal : TPaletter;
  Index : Byte;
Begin
  For Index := 0 to 15 Do Begin
    Pal [ Index ] := Index Mod 8;
    if Index > 13 then
      Pal [ Index ] := ( Index Mod 8 ) + $38;
  End;
  Pal [ 16 ] := 0;
  SetPaletter ( Pal );
End; { Procedure SetSecondFontPaletter }
