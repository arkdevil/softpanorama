Procedure SetPic ( Var Pic; PicSize : Word; YLines : Byte; Fon : Byte );
  { Загрузить в пользовательский массив шрифта картинку }
Type
  TPic = Record
    SX : Word;
    SY : Word;
    Box : Array [ 1..4096 ] of Byte;
  End;
Var
  X, Y, Ind : Word;
Begin
  if PicSize - 4 > 4096 then exit;
  SizeY := 0;
  SizeX := 0;
  FillChar ( LFontArray, SizeOf ( LFontArray ), Chr ( Fon ) );
  SizeX := TPic ( Pic ).SX Div 8;
  if TPic ( Pic ).SX Mod 8 <> 0 then Inc ( SizeX );
  SizeY := TPic ( Pic ).SY Div YLines;
  if TPic ( Pic ).SY Mod YLines <> 0 then Inc ( SizeY );
  X := 1; Y := 1;
  For Y := 1 To SizeY Do  { по строкам }
    For X := 1 To SizeX Do  { по буквам }
      For Ind := 1 To YLines Do  { по линиям в букве }
        if ( Y - 1 )*YLines*SizeX + (Ind-1)*SizeX + X <=
          PicSize - 4 then
            LFontArray [ YLines + ( Y - 1 )*YLines*SizeX +
              ( X - 1 )*YLines + Ind ] :=
            TPic(Pic).Box[ ( Y - 1 )*YLines*SizeX +
              (Ind-1)*SizeX + X ];
End; { Procedure SetPic }
{----------------------------------------------------------}
Function DealLinesPic : Word;
  { Количество строк картинки }
Begin
  DealLinesPic := SizeY;
End; { Function DealLinesPic }
{----------------------------------------------------------}
Function GetPicLine ( Num : Word ) : String;
  { Получить заданную строку картинки в символах }
Var
  Line : String;
  Index : Word;
Begin
  GetPicLine := '';
  if ( Num < 1 ) Or ( Num > SizeY ) then Exit;
  Line := '';
  For Index := ( Num - 1 ) * SizeX  To Num * SizeX - 1 Do
    Line := Line + Chr ( Index + 1 );
  GetPicLine := Line;
End; { Function GetPicLine }
