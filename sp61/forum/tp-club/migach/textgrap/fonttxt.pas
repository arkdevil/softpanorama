FUNCTION GetBlock16Pointer : POINTER;

VAR
   BoxPtr : ^FindArray;
   Index, Hlp : BYTE;

BEGIN
     BoxPtr := ADDR ( Font16 );
     Index := 1;
     Hlp := 0;
     WHILE ( ( Hlp <> 5 ) AND ( Index < 255 ) ) DO
           BEGIN
                IF ( BoxPtr^ [ Index ] = $55 ) THEN
                   INC ( Hlp )
                ELSE
                    Hlp := 0;
                INC ( Index )
           END;
     IF ( Hlp <> 5 ) THEN
        HALT ( 1 )
     ELSE
         GetBlock16Pointer := ADDR ( BoxPtr^[Index] )

END; { FUNCTION GetBlock16Pointer }

