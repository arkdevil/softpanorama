{----------------------------------------------------------}

PROCEDURE LoadFont ( VAR Box; BlockNumber, YSize : BYTE );

          { Загрузка блока шрифта в алфавитно-цифровом режиме }
VAR
   Rg : REGISTERS;

BEGIN
     Rg.AH := $11;
     Rg.AL := 0;
     Rg.BL := BlockNumber;
     Rg.BH := YSize;
     Rg.CX := 256;
     Rg.ES := SEG ( Box );
     Rg.BP := OFS ( Box );
     Rg.DX := 0;
     INTR ( $10, Rg )

END; { PROCEDURE LoadFont }

{----------------------------------------------------------}

PROCEDURE SetActiveFontBlock ( NumBlock1, NumBlock2 : BYTE );

          { Установка активных блоков содержащих необходимый }
          {                  шрифт                           }
VAR
   Rg : REGISTERS;

BEGIN
     IF ( ( NumBlock1 > 3 ) OR ( NumBlock2 > 3 ) ) THEN
        EXIT;
     Rg.AH := $11;
     Rg.AL := 3;
     Rg.BL := NumBlock1 + NumBlock2 * 4;
     INTR ( $10, Rg )

END; { PROCEDURE SetActiveBlock }

{----------------------------------------------------------}

PROCEDURE LoadStandartFont ( BlockNumber, YSize : BYTE );

          { Загрузка стандартного набора символов из ПЗУ }
          {      в один из имеющихся блоков              }
VAR
   Rg : REGISTERS;

BEGIN
     Rg.AH := $11;
     Rg.BL := BlockNumber;
     CASE YSize OF
           8  : Rg.AL := 2;
           14 : Rg.AL := 1;
           16 : Rg.AL := 4
     ELSE
         EXIT
     END;
     INTR ( $10, Rg )

END; { PROCEDURE LoadStandartFont }

