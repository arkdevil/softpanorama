PROCEDURE Border ( Color : BYTE ); ASSEMBLER;

          { Ихзменяет цвет рамки экрана терминала }
ASM
   MOV   AH, $10
   MOV   AL, $01
   MOV   BH, Color
   INT   $10

END; { PROCEDURE Border }

