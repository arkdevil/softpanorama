break off
dos break off
map display off

write
write "Good "; greeting_time; ", "; full_name; "."
write "Logical connection #"; station; " on Server "; file_server; "."

map h:=elmer/sys:\
drive h:

map ins s1:=elmer/sys:\public\netmenu
map ins s2:=elmer/sys:\public\autoexec
map ins s3:=elmer/sys:\public
if member of "RHM" then map ins s4:=elmer/sys:\rhm

map root l:=elmer/sys:usr\%login_name
map root m:=elmer/sys:grp
map root n:=elmer/sys:common
if member of "PURCH"   then  map root m:=elmer/sys:grp\purch
if member of "LIBRARY" then  map root m:=elmer/sys:grp\library
if member of "ACCTNG"  then  map root m:=elmer/sys:grp\acctng
if member of "PAYROLL" then  map root m:=elmer/sys:grp\payroll
if member of "MIS"     then  map root m:=elmer/sys:grp\mis
if member of "LIBRARY" then  map root m:=elmer/sys:grp\library

if member of "PUBLIC-ADVANCED" then begin
     map ins s4:=elmer/sys:apps\extras
     map ins s4:=elmer/sys:public\advanced
     end

map h:=elmer/sys:public\robot
#ncopyscr allusers
#nchkcsys
#nlogaddr h:\public\robot\inbound\login.txt
map h:=elmer/sys:grp\mis\master\shell
#ncopyipx c:\net\ipx.com

dos set name=login_name
dos set rhmmaster="venus/sys:rhm"

map h:=elmer/sys:usr
if member of "MAIN" then begin
     map h:=elmer/sys:public\robot
     #ncopyscr MAIN
     map h:=elmer/sys:usr\%login_name
     end

if login_name = "INSTALL" then begin
     map h:=elmer/sys:grp\mis\master
     end


if login_name = "ANYWHERE" then begin
     exit "Y:startup4.bat"
     end

if member of "NO-NETMENU" then begin
     if not member of "RHM" then begin
          if login_name = "BACKUP" then begin
               exit 
               end
          exit "Y:minimum.bat"
          end
     exit "Y:startup2.bat"
     end

exit "Y:startup.bat"
