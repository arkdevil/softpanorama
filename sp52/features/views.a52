------ arjmark.btm
@echo off
setlocal
ncsfp > nul
if not exist nc.sfl quit
input Enter arj name : %%name
arj a -e -d %name !nc.sfl
del nc.sfl /q
endlocal ------

------ unarjdir.btm
@echo off
setlocal
ncsfp
for %next_name in ( @nc.sfl ) do (
  iff .%@ext[%next_name] == .arj then
    iff not isdir %@name[%next_name] then
      md %@name[%next_name]
    endiff
    pushd %@name[%next_name]
      arj x %next_name -n -v -y
    popd
  else
    echo Bad file selected %next_name
  endiff
)
del nc.sfl /q
endlocal
------

