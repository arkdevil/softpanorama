iff "%1" == "" then
Text

    Convert Decimal to Hex
    Usage :   hex <decimal>

endtext
quit
endiff
Set IntToHex=0
:Loop1
if %IntToHex GE %@LEN[%1] goto convert
iff %@ASCII[%@SUBSTR[%1,%IntToHex,1]] LT 48 .OR.
%@ASCII[%@SUBSTR[%1,%IntToHex,1]] GT 57 then
echo %1
