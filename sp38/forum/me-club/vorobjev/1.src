$MACRO 1;
{Нумерация непустых строк файла}
Def_Int(n := 0);
Refresh := FALSE;
Insert_Mode := TRUE;
Make_Message('Начали...');
tof;
WHILE AT_EOF <> TRUE DO
eol;
if c_col <> 1 then
n := n + 1;
goto_col(1);
	if n > 9 then
	Text(str(n) + '.');
	else
	Text(str(n) + '. ');
	end;
end;
DOWN;
END;
Make_Message('Всё!');
END_MACRO;